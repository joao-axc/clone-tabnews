{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Implementação do https://tabnews.com.br para o https://curso.dev",
  "main": "index.js",
  "scripts": {
    "dev": "echo Docker && npm run services:up && echo Servidor Web && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watchAll"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joao-axc/clone-tabnews.git"
  },
  "author": "Joao Cardoso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joao-axc/clone-tabnews/issues"
  },
  "homepage": "https://github.com/joao-axc/clone-tabnews#readme",
  "dependencies": {
    "next": "13.1.6",
    "pg": "8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "jest": "29.6.2",
    "prettier": "3.6.2"
  }
}
